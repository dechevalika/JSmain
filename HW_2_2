//Задача 2.
console.log(null || 2 || undefined ); // выведется 2, т.к. null - false, 2 - true и это первое истинное значение (или возвращает первое истинное значение)
console.log( 1 && null && 2 ); //выведется null, т.к. 1-true, null-false и это первое ложное значение (и возвращает первое ложное значение)
